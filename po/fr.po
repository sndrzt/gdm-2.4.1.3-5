#, c-format
msgid "gdm_auth_user_add: Could not lock cookie file %s"
msgstr "gdm_auth_user_add : ne peut verrouiller le fichier de cookie %s"

#: daemon/auth.c:500
#, c-format
msgid "%s: Could not write cookie"
msgstr "%s : ne peut écrire de cookie"

