msgid "gdm_child_action: Master suspending..."
msgstr "gdm_child_action : suspension du maître..."

#: daemon/gdm.c:1152
#, c-format
msgid "gdm_child_action: Suspend failed: %s"
msgstr "gdm_child_action : Échec de la suspension : %s"

